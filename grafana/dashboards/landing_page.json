          "rawSql": "select\n'articles in database' as type_of_item,\nto_char(count(id), '999G999G999G999') as number_of_items\nfrom news_meta_data.article_header\nunion\nselect\n'articles added in last 24 hours' as type_of_item,\nto_char(count(id), '999G999G999G999') as number_of_items\nfrom news_meta_data.article_header\nwhere source_date > now() + INTERVAL '-1 day'\nunion\nselect\n'comments in database' as type_of_item,\nto_char(count(id), '999G999G999G999') as number_of_items\nfrom news_meta_data.comment\nunion\nselect\n'comments analyzed' as type_of_item,\nto_char(count(al.comment_id), '999G999G999G999') as number_of_items\nfrom (select distinct comment_id\n\t  from news_meta_data.analyzer1_result) al\nunion\nselect\n'number of sources' as type_of_item,\nto_char(count(id), '999G999G999G999') as  number_of_items\nfrom news_meta_data.source_header\norder by type_of_item",
